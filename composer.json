{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "laravel/framework": "4.2.*",
    "barryvdh/laravel-debugbar": "~1.8",
    "barryvdh/laravel-snappy": "0.1.x",
    "zizaco/testcases-laravel": "dev-master",
    "guilhermeguitte/behat-laravel": "dev-master",
    "phpunit/phpunit": "4.0.*",
    "h4cc/wkhtmltopdf-amd64": "0.12.x",
    "stripe/stripe-php": "2.*",
    "gzero/eloquent-tree": "v2.0",
    "league/csv": "^7.1@dev",
    "eluceo/ical": "*",
    "michelf/php-markdown": "^1.5",
    "bkwld/croppa": "~4.0",
    "phpoffice/phpword": "dev-master",
    "guzzlehttp/guzzle": "~6.0",
    "guzzlehttp/oauth-subscriber": "*",
    "maatwebsite/excel": "~1.3.0",
    "asgrim/ofxparser": "^1.2",
    "twilio/sdk": "^5.0",
    "darkaonline/ripcord": "^0.1.6",
    "clegginabox/pdf-merger": "dev-master",
    "sabre/dav": "3.3.x-dev",
    "ziplr/php-qr-code": "^1.0",
    "google/apiclient": "^2.2",
    "ext-json": "*",
    "ext-curl": "*",
    "pusher/pusher-php-server": "^3.0@dev"
  },
  "autoload": {
    "classmap": [
      "app/commands",
      "app/controllers",
      "app/models",
      "app/models/InvoicingRuleProcessor",
      "app/database/migrations",
      "app/database/seeds",
      "app/tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "SÃ©bastien Hordeaux",
      "email": "sebastien@etincelle-coworking.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://packagist.org"
    },
    { "packagist": false }
  ]
}
